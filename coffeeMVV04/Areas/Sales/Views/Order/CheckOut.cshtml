@using coffeeMVV04.Model.EF;
@model dynamic
@{
    ViewBag.Title = "CheckOut";
    Layout = "~/Areas/Sales/Views/Shared/_Layout.cshtml";
}


<style type="text/css">
    .tg {
        border-collapse: collapse;
        border-spacing: 0;
    }

        .tg td {
            border-color: black;
            border-style: solid;
            border-width: 1px;
            font-family: Arial, sans-serif;
            font-size: 14px;
            overflow: hidden;
            padding: 10px 5px;
            word-break: normal;
        }

        .tg th {
            border-color: black;
            border-style: solid;
            border-width: 1px;
            font-family: Arial, sans-serif;
            font-size: 14px;
            font-weight: normal;
            overflow: hidden;
            padding: 10px 5px;
            word-break: normal;
        }

        .tg .tg-cmh6 {
            border-color: inherit;
            font-family: "Lucida Sans Unicode", "Lucida Grande", sans-serif !important;
            font-size: 20px;
            font-weight: bold;
            text-align: right;
            vertical-align: top
        }

        .tg .tg-2u83 {
            border-color: inherit;
            font-family: "Lucida Sans Unicode", "Lucida Grande", sans-serif !important;
            font-size: 20px;
            font-weight: bold;
            text-align: center;
            vertical-align: top
        }

        .tg .tg-0pky {
            border-color: inherit;
            text-align: left;
            vertical-align: top
        }
</style>
<table class="tg">
    <thead>
        <tr>
            <th class="tg-2u83">ID</th>
            <th class="tg-2u83">ID User</th>
            <th class="tg-2u83">ID Sản phẩm</th>
            <th class="tg-2u83">Date</th>
            <th class="tg-2u83">Số lượng</th>
            <th class="tg-2u83">Trạng thái</th>
        </tr>
    </thead>
    <tbody>
        
            <tr>
                <td class="tg-0pky"></td>
                <td class="tg-0pky"></td>
                <td class="tg-0pky"></td>
                <td class="tg-0pky"></td>
                <td class="tg-0pky"></td>
                <td class="tg-0pky"></td>
            </tr>
            <tr>
                <td class="tg-cmh6" colspan="5">Tổng cộng: </td>
                <td class="tg-0pky">@</td>
            </tr>
        
    </tbody>
</table>


